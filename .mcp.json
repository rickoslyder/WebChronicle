{
  "mcpServers": {
    "cloudflare-browser-rendering": {
      "command": "npx",
      "args": ["-y", "@amotivv/cloudflare-browser-rendering-mcp"],
      "env": {
        "BROWSER_RENDERING_API": "https://activity-log-worker.rickoslyder.workers.dev/browser"
      }
